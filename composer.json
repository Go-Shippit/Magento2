{
    "name": "shippit/magento2",
    "description": "Shippit Integration Module",
    "type": "magento2-module",
    "version": "1.4.18",
    "require": {
        "magento/module-backend": "^100.0.2|^101.0.0",
        "magento/module-config": "^100.0.2|^101.0.0",
        "magento/module-store": "^100.0.2|^101.0.0",
        "magento/module-shipping": "^100.0.2",
        "magento/module-quote": "^100.0.2|^101.0.0",
        "magento/module-checkout": "^100.0.2",
        "php": "^5.5|^5.6|^7.0|^7.1|^7.2"
    },
    "require-dev": {
        "magento/magento-coding-standard": "^4.0"
    },
    "replace": {
        "shippit/shipping": "1.4.17"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Shippit\\Shipping\\": ""
        }
    },
    "prefer-stable": true,
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    }
}
